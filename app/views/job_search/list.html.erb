<% vacancies.each do |vacancy| %>
  <%= vacancy['title'] %> - <%= vacancy['summary'] %> - Vacancy Posted Date: <%= vacancy['activedate']['start'] %> - Vacancy End Date: <%= vacancy['activedate']['end'].to_date %> - <%= link_to vacancy['company'], "https://www.google.co.uk/search?q=#{vacancy['company'].gsub(" ", "+")}" %> - <%= vacancy['location']['postcode'] %> - <%= vacancy['location']['city'] %>
  <hr />
<% end %>